---
layout: null
---
<?xml version='1.0' encoding='UTF-8'?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    <url>
      <loc>{{ site.url }}{{ page.url }}</loc>
      <lastmod>{{ site.time | date: '%Y-%m-%d' }}</lastmod>
      <changefreq>daily</changefreq>
      <priority>1</priority>
    </url>
  {% endfor %}
</urlset>
